	/FIELD=(LASTNAME, TYPE=ASCII, POSITION=2, SEPARATOR=",", SDEF="LASTNAME=B")
	/FIELD=(PSEUDO_FIRSTNAME, TYPE=ASCII, POSITION=3, SEPARATOR=",", SDEF="FIRSTNAME=C", SET="../../Sets/names_first.set")
