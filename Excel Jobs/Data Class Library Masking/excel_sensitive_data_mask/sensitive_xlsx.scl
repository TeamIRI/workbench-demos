	/FIELD=(CITY, TYPE=ASCII, POSITION=5, SEPARATOR=",", SDEF="CITY=E")
	/FIELD=(STATE, TYPE=ASCII, POSITION=6, SEPARATOR=",", SDEF="STATE=F")
	/FIELD=(ENC_FP_ZIP=enc_fp_aes256_alphanum(ZIP, "epass:BpbQuslYrit6VGHKyg51+w=="), TYPE=ASCII, POSITION=7, SEPARATOR=",", SDEF="ZIP=G")
	/FIELD=(ENC_FP_CREDIT_CARD=enc_fp_aes256_alphanum(CREDIT_CARD, "epass:BpbQuslYrit6VGHKyg51+w=="), TYPE=ASCII, POSITION=8, SEPARATOR=",", SDEF="CREDIT_CARD=H")

