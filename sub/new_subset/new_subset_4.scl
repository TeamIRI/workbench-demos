/JOIN NOT_SORTED EMPLOYEES NOT_SORTED ORACLE_ECLIPSE_SCOTT_JOBS WHERE EMPLOYEES.JOB_ID == ORACLE_ECLIPSE_SCOTT_JOBS.JOB_ID

