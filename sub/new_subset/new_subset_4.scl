	/ALIAS=ORACLE_ECLIPSE_SCOTT_JOBS
	/FIELD=(JOB_ID, TYPE=ASCII, POSITION=1, SEPARATOR="\t", ODEF="JOB_ID")
	/FIELD=(JOB_TITLE, TYPE=ASCII, POSITION=2, SEPARATOR="\t", ODEF="JOB_TITLE")
