<?xml version="1.0" encoding="ASCII"?>
<rules:RuleLibrary xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:rules="http://rules/1.0">
  <rules xsi:type="rules:DataRule" name="RedactPinsRule" editor="charReplWizard">
    <properties value="replace_chars(${FIELDNAME},&quot;*&quot;)"/>
  </rules>
  <rules xsi:type="rules:DataRule" name="HashEmailsRule" editor="hashFunctionWizard">
    <properties value="hash_sha2(${FIELDNAME})"/>
  </rules>
</rules:RuleLibrary>
