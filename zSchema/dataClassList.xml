        <description>Class for American date</description>
        <matchers>
            <details>\b([0]\d|[1][012])[-/.]?([012]\d|[3][01])[-/.]?(\d{4})\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>DATE_US_MMDDYYYY</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for email</description>
        <matchers>
            <details>\b[\w._%+-]+@[\w.-]+\.[A-Za-z]{2,4}\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>EMAIL</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for IP address</description>
        <matchers>
            <details>\b((25[0-5]|2[0-4]\d|[01]?\d\d?)\.){3}(25[0-5]|2[0-4]\d|[01]?\d\d?)\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>IP_ADDRESS</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for US Phone</description>
        <matchers>
            <details>\b((\+?(1)([ .-]?)?)?(\(?([2-9]\d{2})\)?([ .-]?)?)([2-9]\d{2})([ .-]?)(\d{4})([ #eExXtT]*)(\d{2,6})?)\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>PHONE_US</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for US Social Security Number</description>
        <matchers>
            <details>\b(\d{3}[-]?\d{2}[-]?\d{4})\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>PIN_US</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for US Zip code</description>
        <matchers>
            <details>\b\d{5}(?:-\d{4})?\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>POSTAL_CODE_US</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for website URL</description>
        <matchers>
            <details>\b(\w+):\/\/([\w\.-@]+)\.([A-Za-z\.]{2,6})([\/\w \(\)\.-]*)*\/?\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>URL</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
    <dataClassInfo>
        <active>true</active>
        <description>Class for Vehicle Identification Number</description>
        <matchers>
            <details>\b([A-HJ-NPR-Z\d]{3})([A-HJ-NPR-Z\d]{5})([\dX])(([A-HJ-NPR-Z\d])([A-HJ-NPR-Z\d])([A-HJ-NPR-Z\d]{6}))\b</details>
            <type>PATTERN</type>
        </matchers>
        <name>VIN_US</name>
        <rank></rank>
        <type>Data Class</type>
    </dataClassInfo>
</dataClassList>
